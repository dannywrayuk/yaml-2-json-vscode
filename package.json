{
  "name": "yaml-json-converter",
  "displayName": "YAML to JSON Converter",
  "description": "Lossless conversion between YAML and JSON",
  "icon": "icon.png",
  "publisher": "dannywrayuk",
  "version": "0.0.12",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/extension.js",
  "activationEvents": [
    "onCommand:yjc.toggleFiletype"
  ],
  "contributes": {
    "commands": [
      {
        "command": "yjc.toggleFiletype",
        "title": "Toggle between YAML and JSON"
      }
    ]
  },
  "scripts": {
    "package": "vsce package",
    "build": "tsc -p ./",
    "dev": "tsc -watch -p ./",
    "test": "jest",
    "test:w": "jest --watch",
    "version": "echo $npm_package_version"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/jest": "^29.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.71.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "jest": "^29.1.2",
    "ts-jest": "^29.0.3",
    "typescript": "^4.7.4",
    "vsce": "^2.13.0"
  },
  "dependencies": {
    "yaml": "^2.1.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dannywrayuk/yaml-json-converter.git"
  }
}
